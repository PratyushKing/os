prompt 0

MENU HIDDEN
MENU AUTOBOOT Booting Rhino Linux OS live disk in # seconds
timeout 30

label live-@FLAVOUR@
	menu label ^Try or install Rhino Linux OS
	menu default
	linux /vmlinuz-6.2.0-okpine-pro
	initrd /initrd.img-6.2.0-okpine-pro
	fdtdir /dtb-6.2.0-okpine-pro/
	append REPTHIS consoleblank=0 loglevel=7 ro quiet splash plymouth.ignore-serial-consoles vt.global_cursor_default=0

label live-failsafe
	menu label Start Rhino Linux OS (Safe Graphics)
	linux /vmlinuz-6.2.0-okpine-pro
	initrd /initrd.img-6.2.0-okpine-pro
	fdtdir /dtb-6.2.0-okpine-pro/
	append REPTHIS consoleblank=0 loglevel=7 ro quiet splash plymouth.ignore-serial-consoles vt.global_cursor_default=0 nomodeset